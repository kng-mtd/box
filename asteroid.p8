pico-8 cartridge // http://www.pico-8.com
version 43
__lua__
function _init()
	me={a=0,x=31,y=63}
	wl1={x=63,y=rnd(112)+16}
	wl2={x=127,y=-rnd(112)}
	pw={x=127,y=rnd(112)+16}
	as={}
	sc=0
	fl=500
	st=true
end


function _update()
 if st then
		if(fl<0) st=false sfx(1)
 
		if(btn(⬆️) and me.y>0) me.y-=2
		if(btn(⬇️) and me.y<120) me.y+=2
		me.a=1-me.a
		
		wl1.x-=1
		if abs(wl1.x-me.x)<4
			and wl1.y-4<me.y then
			 st=false
			 sfx(1)
		end
		if wl1.x<0 then
		 wl1.x=127
		 wl1.y=rnd(112)+16
		 sc+=1
	 end
	
		wl2.x-=1
		if abs(wl2.x-me.x)<4
		 and wl2.y+124>me.y then
		 st=false
		 sfx(1)
	 end
		if wl2.x<0 then
		 wl2.x=127
		 wl2.y=-rnd(112)
		 sc+=1
	 end
	 
	 pw.x-=2
		fl-=1
		if abs(pw.x-me.x)<4
			and abs(pw.y-me.y)<4 then
				pw.y=-8
				fl+=100
				sfx(0)
	 end 
		if pw.x<0 then
		 pw.x=127
		 pw.y=rnd(112)+16
	 end
	 
	 if rnd()<(sc/1000+0.01) then
	 	add(as,{x=127,y=rnd(128)})
	 end
		for i=#as,1,-1 do
 		local a=as[i]
 		a.x-=3
 		if(a.x<0)	del(as,a)

 		if abs(a.x-me.x)<4
 		 and abs(a.y-me.y)<8 then
 		  st=false
 		  sfx(1)
 		end
  end
 end
end

function _draw()
	cls()
	if st then
		spr(4,wl1.x,wl1.y,1,16)
	 spr(4,wl2.x,wl2.y,1,16)
		spr(me.a,me.x,me.y)
	 spr(6,pw.x,pw.y)
	 
	 for i in all(as) do
	 	spr(5,i.x,i.y)
	 end
	 	
		print('score: '..sc
			..'  fuel: '..fl)
 
 else
 	print('game over',15,31)
 	print('score: '..sc
 	 ..' fuel : '..fl)
 	print('ctrl+r to replay')
 end
 
end

